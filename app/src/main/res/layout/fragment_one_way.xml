<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    tools:context=".homeView.Menus.FragmentOneWay">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginStart="20sp"
        android:layout_marginEnd="20dp"
        android:gravity="center"
        android:orientation="horizontal">

        <TextView
            android:id="@+id/tv_gol1"
            android:layout_width="100dp"
            android:layout_height="wrap_content"
            android:layout_marginRight="30sp"
            android:gravity="center"
            android:text="0" />

        <TextView
            android:id="@+id/tv_gol2"
            android:layout_width="100dp"
            android:layout_height="wrap_content"
            android:layout_marginRight="30sp"
            android:gravity="center"
            android:text="0" />

        <TextView
            android:id="@+id/tv_gol3"
            android:layout_width="100dp"
            android:layout_height="wrap_content"
            android:gravity="center"
            android:text="0" />
    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginStart="20dp"
        android:layout_marginEnd="20dp"
        android:gravity="center"
        android:orientation="horizontal">

        <com.google.android.material.button.MaterialButton
            android:id="@+id/golongan_satu"
            android:layout_width="100dp"
            android:layout_height="wrap_content"
            android:layout_marginRight="30sp"
            android:text="Gol - 1"
            app:icon="@drawable/motorcycle"
            app:iconGravity="textTop"
            app:iconSize="50sp" />

        <com.google.android.material.button.MaterialButton
            android:id="@+id/golongan_dua"
            android:layout_width="100dp"
            android:layout_height="wrap_content"
            android:layout_marginRight="30sp"
            android:text="Gol - 2"
            app:icon="@drawable/car"
            app:iconGravity="textTop"
            app:iconSize="50sp" />

        <com.google.android.material.button.MaterialButton
            android:id="@+id/golongan_tiga"
            android:layout_width="100dp"
            android:layout_height="wrap_content"
            android:text="Gol - 3"
            app:icon="@drawable/minibus"
            app:iconGravity="textTop"
            app:iconSize="50sp" />
    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginStart="20sp"
        android:layout_marginEnd="20dp"
        android:gravity="center"
        android:orientation="horizontal">

        <TextView
            android:id="@+id/tv_gol4"
            android:layout_width="100dp"
            android:layout_height="wrap_content"
            android:layout_marginRight="30sp"
