            android:id="@+id/tv_gol5B"
            android:layout_width="100dp"
            android:layout_height="wrap_content"
            android:gravity="center"
            android:text="0" />
    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginStart="20dp"
        android:layout_marginEnd="20dp"
        android:gravity="center"
        android:orientation="horizontal">

        <com.google.android.material.button.MaterialButton
            android:id="@+id/golongan_empat"
            android:layout_width="100dp"
            android:layout_height="wrap_content"
            android:layout_marginRight="30sp"
            android:text="Gol - 4"
            app:icon="@drawable/pickup"
            app:iconGravity="textTop"
            app:iconSize="50sp" />

        <com.google.android.material.button.MaterialButton
            android:id="@+id/golongan_5a"
            android:layout_width="100dp"
            android:layout_height="wrap_content"
            android:layout_marginRight="30sp"
            android:text="Gol - 5a"
            app:icon="@drawable/big_bus"
            app:iconGravity="textTop"
            app:iconSize="50sp" />

        <com.google.android.material.button.MaterialButton
            android:id="@+id/golongan_5b"
            android:layout_width="100dp"
            android:layout_height="wrap_content"
            android:text="Gol - 5b"
            app:icon="@drawable/bus_comm"
            app:iconGravity="textTop"
            app:iconSize="50sp" />
    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginStart="20sp"
        android:layout_marginEnd="20dp"
        android:gravity="center"
        android:orientation="horizontal">

        <TextView
