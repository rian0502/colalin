        <com.google.android.material.button.MaterialButton
            android:id="@+id/golongan_8"
            android:layout_width="100dp"
            android:layout_height="wrap_content"
            android:text="Gol - 8"
            app:icon="@drawable/bicycle"
            app:iconGravity="textTop"
            app:iconSize="50sp" />
    </LinearLayout>
</LinearLayout>